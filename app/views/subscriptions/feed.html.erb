<% if @tweets.size == 0 || @videos.size == 0%>
  Search for users to grow your feed!
<% end %>

<% @all.each do |thing| %>
  <% if thing.inspect[0..8] == "#<Twitter" %>
    <p> <img src= "<%= thing.user.profile_image_url_https %>"></p>
    <p><%= link_to "#{thing.user.screen_name}", "https://twitter.com/intent/user?user_id=#{thing.user.id}" %></p>
    <p><%= thing.user.name %></p>
    <p><%= thing.text %></p>
    <% if thing.user.status.urls? %>
      <%= thing.text[thing.user.status.urls[0].indices[0]] %>
      <a href="<%=thing.user.status.urls[0].expanded_url.to_s %>">test</a>
      <p><%= link_to "#{thing.user.status.urls[0].expanded_url}", thing.user.status.urls[0].expanded_url.to_s %></p>
    <% end %>
    <!--<p><%#= auto_link(thing.text).html_safe %></p>-->
    <p><%= thing.created_at %></p>
    <hr>
  <% else %>
    <p><iframe src="//player.vimeo.com/video/<%=thing['id']%>" width="500" height="400" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe><p><%=thing['title']%> from <%=thing['user_name']%></p>
    <p><%=thing['upload_date']%></p>
    <hr>
  <% end %>
<% end %>
